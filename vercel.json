{
  "version": 2,
  "builds": [
    {
      "src": "ui/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/ui/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/ui/chatbot_ui.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
